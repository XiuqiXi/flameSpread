FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "100";
    object      ILambda_9_0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -3 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    outlet
    {
        type            greyDiffusiveRadiation;
        refValue        uniform 0;
        refGradient     uniform 0;
        valueFraction   uniform 1;
        value           uniform 0;
        emissivityMode  lookup;
        emissivity      uniform 1;
    }
    inlet
    {
        type            greyDiffusiveRadiation;
        refValue        uniform 0;
        refGradient     uniform 0;
        valueFraction   uniform 1;
        value           uniform 0;
        emissivityMode  lookup;
        emissivity      uniform 1;
    }
    wall
    {
        type            greyDiffusiveRadiation;
        refValue        uniform 0;
        refGradient     uniform 0;
        valueFraction   uniform 1;
        value           uniform 0;
        emissivityMode  lookup;
        emissivity      uniform 1;
    }
    interface
    {
        type            greyDiffusiveRadiation;
        refValue        uniform 0;
        refGradient     uniform 0;
        valueFraction   uniform 1;
        value           uniform 0;
        emissivityMode  lookup;
        emissivity      uniform 1;
    }
    frontAndback
    {
        type            empty;
    }
}


